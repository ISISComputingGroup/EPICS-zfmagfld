file zfmagfld_cdaq_data.template {
 pattern 
    {P, AXIS_INDEX, HW_AXIS, USER_AXIS, FWDLINK}

    {"\$(P)", "0", "X", "L", "\$(P)DAQ:Y:_RAW"}
    {"\$(P)", "1", "Y", "T", "\$(P)DAQ:Z:_RAW"}
    {"\$(P)", "2", "Z", "V", "\$(P)MEASURED:X"}

}

file zfmagfld_channelops.template { 
  pattern 
    {P, AXIS_INDEX, HW_AXIS, USER_AXIS, FWDLINK}
    
    {"\$(P)", "0", "X", "L", "\$(P)MEASURED:Y"}
    {"\$(P)", "1", "Y", "T", "\$(P)MEASURED:Z"}
    {"\$(P)", "2", "Z", "V", "\$(P)OVERLOAD"}

}
